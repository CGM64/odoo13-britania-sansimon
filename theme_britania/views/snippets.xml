    <template id="delete_default_copyright_footer" inherit_id="portal.frontend_layout">
        <xpath expr="//div[@id='wrapwrap']/footer/div[@class='o_footer_copyright']" position="replace">
            <div></div>
        </xpath>
    </template>

    <template id="place_into_bar_custom_snippets" inherit_id="website.snippets" name="Visualizacion en la barra">
        <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
            <t t-snippet="theme_britania.snippet_three_buttons_bar"/>
        </xpath>
        <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
            <t t-snippet="theme_britania.snippet_two_col_cards"/>
        </xpath>
        <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
            <t t-snippet="theme_britania.snippet_simple_card"/>
        </xpath>
    </template>
</odoo>