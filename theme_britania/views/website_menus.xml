        </xpath>
        <xpath expr="//li[@t-if='has_visible_submenu']//a" position="replace">
            <a t-if="not submenu.desplegable" class="nav-link" data-toggle="collapse" href="#collapseMotos" aria-expanded="false" aria-controls="collapseMotos" role="button">
                <span t-field="submenu.name"/>
            </a>
            <a t-else="" t-attf-class="#{link_class or ''} dropdown-toggle #{submenu.is_mega_menu and 'o_mega_menu_toggle'}" data-toggle="dropdown" t-att-href="submenu.url or '#'">
