<odoo>
    <record id="website_custom_menu_form" model="ir.ui.view">
        <field name="name">website.custom.menu.form</field>
        <field name="model">website.menu</field>
        <field name="inherit_id" ref="website.website_menus_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//form/sheet/group" position="after">
                <group>
                    <field name="desplegable" />
                    <!--field name="tipo_submenu" /-->
                </group>
            </xpath>
        </field>
    </record>
    <record id="website_custom_menu_tree" model="ir.ui.view">
        <field name="name">website.custom.menu.tree</field>
        <field name="model">website.menu</field>
        <field name="inherit_id" ref="website.menu_tree" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='is_mega_menu']" position="after">
                <field name="desplegable" />
                <!--field name="tipo_submenu" /-->
            </xpath>
        </field>
    </record>
</odoo>