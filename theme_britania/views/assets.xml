<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="britania_assets_frontend" inherit_id="web_editor.assets_frontend" name="Britania Assets" priority="15">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/css" href="/theme_britania/static/src/scss/custom.scss"/>
        </xpath>
        <xpath expr="//script[last()]" position="before">
            <script type="text/javascript" src="/theme_britania/static/src/js/index.js"></script>
        </xpath>
    </template>
</odoo>