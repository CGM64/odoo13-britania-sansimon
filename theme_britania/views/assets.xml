<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="britania_assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/theme_britania/static/src/scss/header.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_britania/static/src/scss/custom.scss"/>
        </xpath>
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/theme_britania/static/src/js/general/index.js" />
        </xpath>
    </template>
    <template id="britania_assets_editor" inherit_id="website.assets_editor">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/theme_britania/static/src/js/editor/custom_options_menu.js" />
        </xpath>
    </template>
</odoo>