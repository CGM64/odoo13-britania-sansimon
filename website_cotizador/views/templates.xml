                        </div>
                    </div>

                    <!--TABS-->
                    <div class="container ">
                        <div class="tab-content" id="myTabContent">
                            <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                                <div class="container py-5 pb-lg-2 mb-lg-3">
                                    <div class="row">
                                        <div class="col-12 col-md-7 contenedor-relativo">
                                            <h1 class="display-3 font-weight-bold text-uppercase wow animate__animated animate__fadeInLeft" data-wow-delay="0.2s" style="text-shadow: 4px 3px 4px rgba(0, 0, 0, 0.5);">for the ride</h1>

                                            <div class="swiper">
                                                <div class="swiper-wrapper">
                                                    <t t-foreach="fleet_vehicle.fleet_model_ids" t-as="img">
                                                        <div class="swiper-slide text-center" t-attf-data-color="{{img.color_name}}" t-field="img.image_vehicle_color" t-options="{'widget': 'image'}" />
                                                    </t>
                                                </div>
                                                <div class="swiper-pagination"></div>
